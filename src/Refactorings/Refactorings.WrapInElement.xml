<?xml version="1.0" encoding="utf-8"?>
<Refactorings>
  <Refactoring Id="RRX001" Identifier="WrapInElement" Title="Wrap in element">
    <Syntaxes>
      <Syntax>selected word(s) in documentation comment</Syntax>
    </Syntaxes>
    <Samples>
      <Sample>
        <Before><![CDATA[/// <summary>
/// null
/// </summary>
public class Foo
{
}
]]></Before>
        <After><![CDATA[/// <summary>
/// <c>null</c>
/// </summary>
public class Foo
{
}]]></After>
      </Sample>
    </Samples>
  </Refactoring>
</Refactorings>